{
  "include": ["src"],
  "exclude": ["node_modules", "prisma"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,

    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,


    "strict": true,
    "skipLibCheck": true
  }
}
